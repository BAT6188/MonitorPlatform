                    </Grid>

                </Border>


                <Border Grid.Row="1" Style="{StaticResource BigPopupSection}" Margin="10,5,10,20" Padding="10,20,10,20">
                    <Grid >
                        <Grid.RowDefinitions>
                            <RowDefinition  Height="50"></RowDefinition>
                            <RowDefinition  Height="*"></RowDefinition>
                            <RowDefinition  Height="*"></RowDefinition>
                            <RowDefinition  Height="*"></RowDefinition>
                            <RowDefinition  Height="*"></RowDefinition>
                            <RowDefinition  Height="*"></RowDefinition>
                            <RowDefinition  Height="*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0" Style="{StaticResource PopupContent}"  Width="220" Margin="0,0,0,20" >
                            <StackPanel HorizontalAlignment="Center"  VerticalAlignment="Center"  Orientation="Horizontal">
                                <Border CornerRadius="5"   Background="Red" Width="20" Height="20">
                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"  Style="{StaticResource BaseContentWhiteStyle}"   Text="2"  ></TextBlock>
                                </Border>
                                <TextBlock Style="{StaticResource BaseContentWhiteStyle}"  Text=" 号线"></TextBlock>
                            </StackPanel>
                        </Border>
                    

                        <TextBlock Grid.Row="1" Style="{StaticResource BaseContentTextStyle}">
                            <Run  Foreground="White"  Text="     宝带桥南 " />
                            <Run  Text="首班时间: " />
                            <Run  Foreground="White"  Text="{Binding Path=SubWayLines[1].UptrainStartTime}" />
                        </TextBlock>
                        <TextBlock Grid.Row="2" Style="{StaticResource BaseContentTextStyle}">
                            <Run  Foreground="White"  Text="     宝带桥南 " />
                            <Run  Text="末班时间: " />
                            <Run  Foreground="White"  Text="{Binding Path=SubWayLines[1].UptrainEndTime}" />
                        </TextBlock>

                        <TextBlock Grid.Row="3" Style="{StaticResource BaseContentTextStyle}">
                            <Run  Foreground="White"  Text="     苏州北站 " />
                            <Run  Text="首班时间: " />
                            <Run  Foreground="White"  Text="{Binding Path=SubWayLines[1].DowntrainStartTime}" />
                        </TextBlock>
                        <TextBlock Grid.Row="4" Style="{StaticResource BaseContentTextStyle}">
                            <Run  Foreground="White"  Text="     苏州北站 " />
                            <Run  Text="末班时间: " />
                            <Run  Foreground="White"  Text="{Binding Path=SubWayLines[1].DowntrainEndTime}" />
                        </TextBlock>


                        <TextBlock Grid.Row="5" Style="{StaticResource BaseContentTextStyle}">
                             <Run  Text="     拥挤站点: " />
                            <Run  Foreground="White"  Text="{Binding Path=SubWayLines[1].CrowdStation}" />
                            <Run  Text="个" />
                        </TextBlock>
                        <TextBlock Grid.Row="6" Style="{StaticResource BaseContentTextStyle}">
                             <Run  Text="     拥堵站点: " />
                            <Run  Foreground="White"  Text="{Binding Path=SubWayLines[1].BlockStation}"  />
                            <Run  Text="个" />
                        </TextBlock>
                    </Grid>

                </Border>

            </Grid>

        </Border>
        <Popup x:Name="stationinfo" PopupAnimation="Fade"  AllowsTransparency ="True" Placement="MousePoint">
            <Border Padding="10,15,10,10" Style="{StaticResource BigPopupSection}" Height="140" >
                <Grid  >
                    <Grid.RowDefinitions>
                        <RowDefinition  Height="*"></RowDefinition>
                        <RowDefinition  Height="*"></RowDefinition>
                        <RowDefinition  Height="*"></RowDefinition>
                        <RowDefinition  Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <StackPanel Grid.Row="0"  Orientation="Horizontal">
                        <Border x:Name="sublineBorder"  CornerRadius="5"   Background="Green" Width="20" Height="20">
                            <TextBlock x:Name="sublinename"  HorizontalAlignment="Center" VerticalAlignment="Center"  Style="{StaticResource BaseContentWhiteStyle}"   Text="1"  ></TextBlock>
                        </Border>
                        <TextBlock Style="{StaticResource BaseContentWhiteStyle}"  Text=" 号线"></TextBlock>
                    </StackPanel>
                    <TextBlock Grid.Row="1" x:Name="stationName"  Style="{StaticResource BaseContentTextStyle}" Text="钟南街">

                    </TextBlock>
                    <TextBlock Grid.Row="2" Style="{StaticResource BaseContentTextStyle}">
                             <Run  Text="当前进站人数: " />
                            <Run  x:Name="inNumber"  Foreground="White"  Text="5000" />
                    </TextBlock>
                    <TextBlock Grid.Row="3" Style="{StaticResource BaseContentTextStyle}">
                             <Run  Text="当前出站人数: " />
                            <Run x:Name="outNumber"  Foreground="White"  Text="5000" />
                    </TextBlock>
                </Grid>


            </Border>

        </Popup>
    </Grid>
    
    
</Page>

    